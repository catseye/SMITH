; Hello, world in SMITH (pathological)
; R0 -> index into string (starts at R10)
; R2 -> -1
  MOV R0, 1
  MOV R[R0], "Hello, world!"
  MOV TTY, R1
  MOV TTY, R2
  MOV TTY, R3
  MOV TTY, R4
  MOV TTY, R5
  MOV TTY, R6
  MOV TTY, R7
  MOV TTY, R8
  MOV TTY, R9
  MOV TTY, R10
  MOV TTY, R11
  MOV TTY, R12
  MOV TTY, R13
